{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "space-before-function-paren": "off",
    "react/prop-types": "off",
    "import/extensions": ["error", "ignorePackages", { "": "never" }],
    "import/no-unresolved": "off",
    "object-curly-newline": "off"
  }
}
